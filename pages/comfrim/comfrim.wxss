  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.icon-img{
  width:90%;
}
.btn{
  width:100%;
  padding:30rpx 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.btn-detail{
  width:90%;
