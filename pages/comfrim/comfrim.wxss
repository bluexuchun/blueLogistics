  flex-direction: row;
}
.line-label{
  width:30%;
}
.line-picker{
  width:70%;
}
.line-start{
  border-bottom:1px solid #F2F2F2;
}
.line-tools{
  width:90%;
  height:80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.tols-add,.tols-delete{
  width:50%;
  height:100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
