  flex-direction: row;
  align-items:center;
  font-size:32rpx;
  color:#6B6B6B;
  position: relative;
}
.icon-item:after{
  content:"";
  width:90%;
  height:1px;
  background: #F2F2F2;
  position: absolute;
  bottom:0px;
  right:-1%;
}

.icon-item-spe{
  padding:30rpx 25rpx;
