.comfrim{
  width:100%;
  background: #fff;
  border-top:1px solid #F2F2F2;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.comfrim-item{
  width:95%;
  min-height:90rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom:1px solid #F2F2F2;
}
.comfrim-item-spe{
  width:95%;
  border-bottom:1px solid #F2F2F2;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding:30rpx 0rpx;
}
.comfrim-item-spe .comfrim-label{
  width:100%;
  margin-right:0px;
}
.line-lists{
  width:100%;
  box-sizing: border-box;
  padding:16rpx;
