page{
  background: #fff
}
.comfrim{
  width:100%;
  background: #fff;
  border-top:1px solid #F2F2F2;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.comfrim-item{
  width:95%;
  min-height:90rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom:1px solid #F2F2F2;
}
.comfrim-label{
  margin-right:40rpx;
  font-size:30rpx;
  color:#6B6B6B;
}
.comfrim-input{
  font-size:28rpx;
  color:#C3CAD3;
}
.inputPlace{
  font-size:28rpx;
  color:#C3CAD3;
}
.imgLists{
  width:100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.img-item{
  width:50%;
  padding:30rpx 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.icon-img{
  width:90%;
}
.btn{
  width:100%;
  padding:30rpx 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.btn-detail{
  width:90%;
  background: #298CFF;
  height:80rpx;
  border-radius: 30px;
  text-align: center;
  line-height: 80rpx;
  color:#fff;
}
.icon-camera{
  width:50rpx;
}
.comfrim-input{
  width:60%;
}