/* mt_picture */
.img-pro{
  width:100%;
  padding:0rpx 16rpx;
  box-sizing: border-box;
}
.img-pro .imgpro-title{
  width:100%;
  font-size:30rpx;
  color:#6B6B6B;
  height:80rpx;
  line-height:80rpx;
  border-bottom:1px solid #F2F2F2;
}
.imgpro-add{
  width:100%;
  min-height:200rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border:1px dashed #289ffa;
  border-radius: 6px;
  margin:40rpx 0rpx;
}
.imgpro-add image{
  width:80rpx;
}
.imgpro-picture{
  width:100%;
  display: flex;
  flex-direction: column;
  padding:30rpx 0rpx;
  border-bottom:1px solid #F2F2F2;

}
.imgpro-picitem{
  width:100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  position: relative;
}
.imgpro-picitem image{
  width:65%;
  display: block;
}
.imgpro-delete{
  width:30%;
  position: absolute;
  right: 0px;
  top:0px;
  height:100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size:28rpx;
  color:#6B6B6B;
  border-left:1px solid #F2F2F2;
}
.imgpro-delete image{
  width:40rpx;
}
.publish-item{
